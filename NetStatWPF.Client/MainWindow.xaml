<Window x:Class="NetStatWPF.Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NetStatWPF.Client"
        mc:Ignorable="d"
        Title="NetStat traffic history" Height="450" Width="800">
    <Grid>
        <GroupBox Header="GroupBox" Margin="10,10,10,35">
            <TextBlock x:Name="Output" Margin="0,0,-2,1" TextWrapping="Wrap" Text="TextBlock"/>
        </GroupBox>
        <Button x:Name="GetBtn" Content="Get" Margin="10,0,0,10" Click="GetBtn_Click" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="75"/>
        <Button x:Name="CloseBtn" Content="Close" Margin="0,0,10,10" Click="CloseBtn_Click" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75"/>
    </Grid>
</Window>
